<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jaydeep.blesample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="27"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jaydeep.blesample.operation.TimerPages.TimerMenuActivity"
            android:exported="true"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="com.jaydeep.blesample.operation.TransitionPageFav"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.jaydeep.blesample.Slider.TempActivity"
            android:exported="true" />
        <activity
            android:name="com.jaydeep.blesample.operation.TransationPage"
            android:exported="true"
            android:screenOrientation="landscape" >
        </activity>
        <activity android:name="com.jaydeep.blesample.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jaydeep.blesample.operation.OperationActivity" />
    </application>

</manifest>